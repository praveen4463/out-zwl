# prompts test
staticSitePrefix = staticSite.urlPrefix
#openUrl("https://jsfiddle.net/praveen4463/0c58wzsx/")
openUrl(staticSitePrefix + "prompts_alerts_c.html")
switchFrame("iframe[name='result']")
confirmValue = findElement("#confirmvalue")
promptValue = findElement("#promptmvalue")
alert = findElement("#alert")
confirm = findElement("#confirm")
prompt = findElement("#prompt")

click(alert)
untilAlertPresent()
acceptAlert()
click(alert)
dismissAlert()

click(alert)
assertTrue(getAlertText() == "this is an alert")
acceptAlert()

click(confirm)
dismissAlert()
assertTrue(getElementValue(confirmValue) == "false")
click(confirm)
acceptAlert()
assertTrue(getElementValue(confirmValue) == "true")

click(prompt)
dismissAlert()
assertTrue(getElementValue(promptValue) == "")
click(prompt)
acceptAlert()
assertTrue(getElementValue(promptValue) == "")
click(prompt)
sendAlertText("33")
dismissAlert()
assertTrue(getElementValue(promptValue) == "")
click(prompt)
sendAlertText("35")
acceptAlert()
assertTrue(getElementValue(promptValue) == "35")