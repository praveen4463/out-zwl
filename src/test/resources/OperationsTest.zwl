# Tests arithmetic, logical and other type of operations supported in ZWL.

# logical
assert(true || false)
assert(!false)
assert(true && true)
assert(!true || !false)
assert(true && (true || false))

# relational expressions
assert(1 < 2)
assert(666 >= 666)
assert(-5 > -6)
assert(0 >= -1)
assert(2 <= 2)
assert(1 <= 2)
assert(1 != 2)
assert(2 == 2)

# add
assert(1 + 999 == 1000)
assert(2 - -2 == 4)
assert(-1 + 1 == 0)
assert(1 - 50 == -49)
assertEqual(33, parseNum('11') + parseNum('22'))

# multiply
assert(3 * 50 == 150)
assert(4 / 2 == 2)
assert(1 / 4 == 0.25)
assert(999999 % 3 == 0)
assert(-5 * -5 == 25)
assert(1 + 2 + 3 * 5 / 4 == 6.75)
assert((1 + 2 + 3) * 5 / 4 == 7.5)